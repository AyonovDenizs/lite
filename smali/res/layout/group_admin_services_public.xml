<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffebedf0">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="-4.0dip">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8.0dip"
            android:orientation="vertical">

            <CheckBox
                android:id="@id/group_service_wall"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginTop="8.0dip"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/group_enable_comments"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_links"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/group_links"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_photos"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/group_service_photoalbums"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_videos"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/videos"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_audios"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/audios"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_board"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/board"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_events"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/public_events"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_places"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/places"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />

            <CheckBox
                android:id="@id/group_service_contacts"
                android:layout_width="fill_parent"
                android:layout_height="48.0dip"
                android:layout_marginLeft="@dimen/post_side_padding"
                android:layout_marginRight="@dimen/post_side_padding"
                android:layout_marginBottom="8.0dip"
                android:drawableLeft="@android:color/transparent"
                android:drawablePadding="16.0dip"
                android:gravity="center|left"
                android:singleLine="true"
                android:text="@string/group_contacts"
                android:textColor="#ff212121"
                android:textSize="16.0dip" />
        </LinearLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>